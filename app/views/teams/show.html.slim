= render 'teams/partials/team_details'
= render 'teams/partials/team_users'
= render 'teams/partials/namespaces'

= render 'teams/partials/components_templates'
= render 'namespaces/partials/components_templates'

- content_for :js_header do
  javascript:
    window.teamMembers = #{raw @team_users_serialized.to_json};
    window.teamNamespaces = #{raw @team_namespaces_serialized.to_json};
    window.availableRoles = #{raw @available_roles.to_json};
    window.manageTeamsEnabled = #{manage_teams_enabled?};
    window.teamsPath = '#{teams_path}';
